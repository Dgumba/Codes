int photores = A0;
int leds[6] = {2,3,4,5,6,7};

void setup()
{
  pinMode(photores, INPUT);
  for (int i = 0; i < 6; i++)
    pinMode(leds[i], OUTPUT);
  Serial.begin(9600);
}

void loop()
{
  int data = analogRead(photores);
  Serial.println(data);
  int el = map(data, 150, 500, 6, 0);
  for (int i = 0; i < el; i++){
    digitalWrite(i, 1);
  	digitalWrite(i, 0);
  }
}
